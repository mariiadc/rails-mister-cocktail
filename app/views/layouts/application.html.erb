  <footer>
    <div class="footer py-3 my-0">
      <div class="container text-center py-3">
        <div class="links my-2">
          <%= link_to '', 'https://www.linkedin.com/in/mariiadcampos/', class: "fab fa-linkedin"  %>
          <%= link_to '', 'https://github.com/mariiadc', class: "fab fa-github-square"  %>
        </div>
